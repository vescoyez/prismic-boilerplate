  "scripts": {
    "dev": "gulp dev --require @babel/register --gulpfile gulp",
    "build": "gulp build --require @babel/register --gulpfile gulp"
  },
  "engines": {
    "node": ">= 13.13.0"
  },
  "browserslist": [
    "> 0.25%"
  ],
  "eslintConfig": {
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "extends": "eslint:recommended",
    "rules": {
      "no-console": "warn"
    },
    "parserOptions": {
      "ecmaVersion": 9,
      "sourceType": "module"
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  }
}
